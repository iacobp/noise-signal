{
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": ["error", { 
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_" 
    }],
    "import/no-unresolved": "off",
    "no-undef": "off"
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/"]
} 